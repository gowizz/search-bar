{
  "name": "@gowiz/searchbar",
  "version": "0.2.3",
  "description": "Different search bars powered by Gowiz search engine technology",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc --declaration",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint:css": "npx stylelint \"**/*.css\" --fix",
    "format": "prettier --write \"**/*.+(json|md|ts|tsx)\"",
    "prepare": "npm dedupe"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gowizz/SearchBar.git"
  },
  "readme": "https://github.com/gowizz/SearchBar/blob/master/README.md",
  "license": "UNLICENSED",
  "author": "Karl-Gustav Kallasmaa (https://github.com/KGKallasmaa)",
  "keywords": [
    "gowiz",
    "search",
    "engine",
    "searchbar",
    "onsite"
  ],
  "bugs": {
    "url": "https://github.com/gowizz/SearchBar/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__test__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|ts?)$"
  },
  "pre-commit": [
    "lint:css",
    "format"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.35",
    "@types/react-addons-shallow-compare": "^0.14.22",
    "fuse.js": "^6.4.6",
    "jest": "^27.3.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-addons-shallow-compare": "^15.6.3",
    "react-device-detect": "^2.1.2",
    "react-dom": "^17.0.2",
    "stylelint": "^14.0.1",
    "stylelint-config-standard": "^24.0.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2",
    "uuid": "^8.3.2",
    "axios": "^0.24.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  }
}
